_direct_link_selectors":[{"infotype":"Advanced Analytics","title":"R Enterprise Installation and Administration Guide","html_link":"http://docs.oracle.com/cd/E67822_01/OREAD/toc.htm","pdf_link":"http://docs.oracle.com/cd/E67822_01/OREAD/E49658-04.pdf","epub_link":"http://docs.oracle.com/cd/E67822_01/OREAD/E49658-04.epub","mobi_link":"http://docs.oracle.com/cd/E67822_01/OREAD/E49658-04.mobi"},{"infotype":"Advanced Analytics","title":"R Enterprise Release Notes","html_link":"http://docs.oracle.com/cd/E67822_01/ORERN/toc.htm","pdf_link":"http://docs.oracle.com/cd/E67822_01/ORERN/E49659-04.pdf","epub_link":"http://docs.oracle.com/cd/E67822_01/ORERN/E49659-04.epub","mobi_link":"http://docs.oracle.com/cd/E67822_01/ORERN/E49659-04.mobi"},{"infotype":"Advanced Analytics","title":"R Enterprise User's Guide","html_link":"http://docs.oracle.com/cd/E67822_01/OREUG/toc.htm","pdf_link":"http://docs.oracle.com/cd/E67822_01/OREUG/E49688-07.pdf","epub_link":"http://docs.oracle.com/cd/E67822_01/OREUG/E49688-07.epub","mobi_link":"http://docs.oracle.com/cd/E67822_01/OREUG/E49688-07.mobi"}],"book_data":[{"title":"Data Mining API Guide","description":"Provides reference information on Oracle Data Mining - Introduction, Using API, Data Mining API reference ","category":"Advanced Analytics","pdf":"dmapi/data-mining-api-guide.pdf","html":"dmapi/index.html"},{"title":"Data Mining Concepts","description":"Discusses the basic concepts underlying Oracle Data Mining. Includes an overview of the features of Oracle Data Mining and information about mining functions and algorithms.","category":"Advanced Analytics","pdf":"dmcon/data-mining-concepts.pdf","html":"dmcon/index.html"},{"title":"Data Mining User's Guide","description":"Explains how to use the SQL interface to Oracle Data Mining to create models and score data. This Guide also addresses administrative issues such as security, import/export, and upgrade for Oracle Data Mining.","category":"Advanced Analytics","pdf":"dmprg/data-mining-users-guide.pdf","html":"dmprg/index.html"},{"title":"Database Data Warehousing Guide","description":"Provides conceptual, reference, and implementation material for using Oracle Database in data warehousing. It covers the full range of data warehousing activities, from physical database design to advanced calculation techniques.","category":"Essentials","pdf":"dwhsg/database-data-warehousing-guide.pdf","html":"dwhsg/index.html"},{"title":"Database Performance Tuning Guide","description":"Provides information about how to tune the performance of Oracle Database using Oracle performance tools.","category":"Essentials","pdf":"tgdba/database-performance-tuning-guide.pdf","html":"tgdba/index.html"},{"title":"Database Utilities","description":"Describes how to use Oracle Database utilities to load data into a database, transfer data between databases, and maintain data. The topics discussed include Data Pump Export, Data Pump Import, SQL*Loader, external tables and associated access drivers, the Automatic Diagnostic Repository Command Interpreter (ADRCI), DBVERIFY, DBNEWID, LogMiner, the Metadata API, original Export, and original Import.","category":"Essentials","pdf":"sutil/database-utilities.pdf","html":"sutil/index.html"},{"title":"OLAP DML Reference","description":"This publication documents the Oracle OLAP DML language.","category":"Online Analytical Processing (OLAP)","pdf":"oladm/olap-dml-reference.pdf","html":"oladm/index.html"},{"title":"OLAP Expression Syntax Reference","description":"Describes the Oracle OLAP expression syntax used to generate information-rich content from the data stored in dimensional database objects such as cubes and cube dimensions.","category":"Online Analytical Processing (OLAP)","pdf":"olaxs/olap-expression-syntax-reference.pdf","html":"olaxs/index.html"},{"title":"OLAP Java API Developer's Guide","description":"Describes how to use the OLAP Java API.","category":"Online Analytical Processing (OLAP)","pdf":"olaap/olap-java-api-developers-guide.pdf","html":"olaap/index.html"},{"title":"OLAP User's Guide","description":"Explains how SQL applications can extend their analytic processing capabilities and manage summary data by using the OLAP option of Oracle Database.","category":"Online Analytical Processing (OLAP)","pdf":"olaug/olap-users-guide.pdf","html":"olaug/index.html"},{"title":"Oracle OLAP Java API Reference","description":"Provides reference information for the Oracle OLAP Java API.","category":"Online Analytical Processing (OLAP)","html":"olapi/index.html"},{"title":"R Enterprise Installation and Administration Guide","html":"../../../../../cd/E67822_01/OREAD/toc.htm","pdf":"../../../../../cd/E67822_01/OREAD/E49658-04.pdf","mobi":"../../../../../cd/E67822_01/OREAD/E49658-04.mobi","epub":"../../../../../cd/E67822_01/OREAD/E49658-04.epub","category":"Advanced Analytics"},{"title":"R Enterprise Release Notes","html":"../../../../../cd/E67822_01/ORERN/toc.htm","pdf":"../../../../../cd/E67822_01/ORERN/E49659-04.pdf","mobi":"../../../../../cd/E67822_01/ORERN/E49659-04.mobi","epub":"../../../../../cd/E67822_01/ORERN/E49659-04.epub","category":"Advanced Analytics"},{"title":"R Enterprise User's Guide","html":"../../../../../cd/E67822_01/OREUG/toc.htm","pdf":"../../../../../cd/E67822_01/OREUG/E49688-07.pdf","mobi":"../../../../../cd/E67822_01/OREUG/E49688-07.mobi","epub":"../../../../../cd/E67822_01/OREUG/E49688-07.epub","category":"Advanced Analytics"},{"title":"SQL Tuning Guide","description":"Describes how to perform SQL tuning, which is the iterative process of improving SQL statement performance to meet specific, measurable, and achievable goals.","category":"Essentials","pdf":"tgsql/sql-tuning-guide.pdf","html":"tgsql/index.html"},{"title":"VLDB and Partitioning Guide","description":"Contains an overview of very large database (VLDB) topics, with emphasis on partitioning and parallel execution as a key component of the VLDB strategy.","category":"Essentials","pdf":"vldbg/vldb-and-partitioning-guide.pdf","html":"vldbg/index.html"}]}